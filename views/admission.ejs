<div class="container1">
        <table class="table" border='1'>
                <tr class="thread.bg-primary">
                    <td>Registration ID</td>
                    <td>Date of Enrollment</td>
                    <td>Batch ID</td>
                </tr>
            <%
            if(admissionData.length!=0){
            admissionData.forEach(data => {
            %>
                <tr>
                    <td><%=data.registration_id %></td>
                    <td><%=data.date_of_enrollment %></td>
                    <td><%=data.batch_id %></td>
                    
                </tr>
            <% }) %>

            <% } else{ %>
                <tr>
                    <td colspan="7">No Data Found</td>
                </tr>
            <% } %>

        </table>
        <a class="btn btn-outline-secondary btn-lg" href="/" role="button" style="margin-left: 45%; background-color:#EB6864; color: #ffffff; margin-top:5%">Home</a>
</div>
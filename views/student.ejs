<h1><%=title%></h1>

<!-- Kartik tu yaha changes kar sakta hai tereko agar koi bootstrap configure karna ho fir-->
<div class="container">
        <table border='1'>
                <tr>
                    <td>Registration ID</td>
                    <td>Name</td>
                    <td>Address</td>
                    <td>Date of Birth</td>
                    <td>Contact No.</td>
                    <td>Email ID</td>
                    <td>Gender</td>
                </tr>
            <%
            if(studentData.length!=0){
            studentData.forEach(data => {
            %>
                <tr>
                    <td><%=data.registration_id %></td>
                    <td><%=data.name %></td>
                    <td><%=data.address %></td>
                    <td><%=data.date_of_birth %></td>
                    <td><%=data.contact_no %></td>
                    <td><%=data.email_id %></td>
                    <td><%=data.gender %></td>
                </tr>
            <% }) %>

            <% } else{ %>
                <tr>
                    <td colspan="7">No Data Found</td>
                </tr>
            <% } %>

        </table>
</div>
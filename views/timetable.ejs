<h1><%=title%></h1>

<!-- Kartik tu yaha changes kar sakta hai tereko agar koi bootstrap configure karna ho fir-->
<div class="container">
        <table border='1'>
                <tr>
                    <td>Lecture ID</td>
                    <td>Day</td>
                    <td>Timing</td>
                    <td>Batch ID</td>
                </tr>
            <%
            if(timetableData.length!=0){
            timetableData.forEach(data => {
            %>
                <tr>
                    <td><%=data.lecture_id %></td>
                    <td><%=data.day %></td>
                    <td><%=data.timing %></td>
                    <td><%=data.batch_id %></td>
                </tr>
            <% }) %>

            <% } else{ %>
                <tr>
                    <td colspan="7">No Data Found</td>
                </tr>
            <% } %>

        </table>
        <a class="btn btn-outline-secondary btn-lg" href="/" role="button">Home</a>
</div>
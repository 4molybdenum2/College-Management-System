<header>
    <nav class="navbar navbar-expand-md navbar-dark fixed-top bg-primary">
        <h1 class="navbar-brand mx-auto ">College Management System</h1>
</header>
<div class="container2" style="margin-top:10%;">
        <table class="table" border='1'>
            <tr  class="thread.bg-primary">

                    <td>Lecture ID</td>
                    <td style=>Day</td>
                    <td>Timing</td>
                    <td>Batch ID</td>
                </tr>
            <%
            if(timetableData.length!=0){
            timetableData.forEach(data => {
            %>
                <tr>
                    <td><%=data.lecture_id %></td>
                    <td><%=data.day %></td>
                    <td><%=data.timing %></td>
                    <td><%=data.batch_id %></td>
                </tr>
            <% }) %>

            <% } else{ %>
                <tr>
                    <td colspan="7">No Data Found</td>
                </tr>
            <% } %>

        </table>
        <a class="btn btn-outline-secondary btn-lg" href="/" role="button" >Home</a>
</div>
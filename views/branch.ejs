<header>
    <nav class="navbar navbar-expand-md navbar-dark fixed-top bg-primary">
        <h1 class="navbar-brand mx-auto ">College Management System</h1>
</header>
<div class="container3" style="margin-top:10%;margin-left:5%;">
        <table class="table" border='1'>
            <tr  class="thread.bg-primary">

                    <td>Branch ID</td>
                    <td>Branch Name</td>
                    <td>Course Duration</td>
                    <td>Degree Granted</td>
                    <td>Seats</td>
                </tr>
            <%
            if(branchData.length!=0){
            branchData.forEach(data => {
            %>
                <tr>
                    <td><%=data.branch_id %></td>
                    <td><%=data.branch_name %></td>
                    <td><%=data.course_duration %></td>
                    <td><%=data.degree_granted %></td>
                    <td><%=data.seats %></td>
                </tr>
            <% }) %>

            <% } else{ %>
                <tr>
                    <td colspan="7">No Data Found</td>
                </tr>
            <% } %>

        </table>
        <a class="btn btn-outline-secondary btn-lg" href="/" role="button" style="margin-left: 45%; background-color:#EB6864; color: #ffffff; margin-top:5%">Home</a></div>